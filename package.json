{
  "name": "anomalies",
  "version": "1.0.0",
  "description": "Detecting anomalies frontend",
  "scripts": {
    "test": "jest",
    "analyze": "webpack --config webpack/analyze.config.js --mode production",
    "build": "webpack --config webpack/prod.config.js --mode production",
    "dev": "webpack-dev-server --config webpack/dev.config.js --mode development --open",
    "bd-server-build": "tsc -p tsconfig-server.json && npm run bd-server-copy-static-assets",
    "bd-server-serve": "cd backless-development/server && node index.js",
    "bd-server-copy-static-assets": "ts-node ./src/server/copy-static-assets.ts"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/carldata/anomaly-tool.git"
  },
  "keywords": [],
  "author": "",
  "jest": {
    "moduleFileExtensions": [
      "ts",
      "tsx",
      "js"
    ],
    "transform": {
      "^.+\\.(ts|tsx)$": "<rootDir>/jest-preprocessor.js"
    },
    "testMatch": [
      "**/__tests__/unit/**.(ts|tsx|js)",
      "**/__tests__/schemas/**.(ts|tsx|js)"
    ]
  },
  "license": "Apache-2.0",
  "bugs": {
    "url": "https://github.com/carldata/anomaly-tool/issues"
  },
  "homepage": "https://github.com/carldata/anomaly-tool#readme",
  "devDependencies": {
    "@types/moment": "^2.13.0",
    "@types/react-datepicker": "^1.1.4",
    "@types/react-router-bootstrap": "^0.24.4",
    "@types/express": "^4.11.1",
    "@types/jest": "^23.1.0",
    "@types/lodash": "^4.14.108",
    "@types/papaparse": "^4.1.34",
    "@types/react": "^16.3.13",
    "@types/react-bootstrap": "^0.32.9",
    "@types/react-data-grid": "^2.0.10",
    "@types/react-dom": "^16.0.5",
    "@types/react-redux": "^5.0.19",
    "@types/react-router": "^4.0.24",
    "@types/react-router-dom": "^4.2.6",
    "@types/react-router-redux": "^5.0.13",
    "@types/redux-actions": "^2.2.4",
    "@types/yup": "^0.24.6",
    "copy-webpack-plugin": "^4.5.1",
    "css-loader": "^0.28.11",
    "file-loader": "^1.1.11",
    "jest": "^23.1.0",
    "node-sass": "^4.9.0",
    "sass-loader": "^6.0.7",
    "shelljs": "^0.8.2",
    "style-loader": "^0.20.3",
    "ts-loader": "^4.2.0",
    "ts-node": "^6.1.2",
    "tslint": "^5.9.1",
    "typescript": "^2.9.2",
    "webpack": "^4.6.0",
    "webpack-bundle-analyzer": "^2.11.1",
    "webpack-cli": "^2.1.2",
    "webpack-dev-server": "^3.1.3",
    "webpack-merge": "^4.1.2"
  },
  "dependencies": {
    "axios": "^0.18.0",
    "date-fns": "^1.29.0",
    "express": "^4.16.3",
    "immutability-helper": "^2.7.0",
    "lodash": "^4.17.10",
    "moment": "^2.22.2",
    "papaparse": "^4.4.0",
    "react": "^16.3.1",
    "react-bootstrap": "^0.32.1",
    "react-csv": "^1.0.14",
